@using RASmartOffice.Core.Models
@model List<MeetingRoomObserveInfo>

<table id="ObserveDataTbl" class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Name</th>
            <th>Description</th>
            <th>Floor</th>
            <th>Capacity</th>
            <th>Status</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var observeInfo in Model)
        {
            <tr data-mrid="@observeInfo.ID">
                <td>@observeInfo.Name</td>
                <td>@observeInfo.Description</td>
                <td>@observeInfo.Floor</td>
                <td>@observeInfo.Capacity</td>
                <td>@observeInfo.Status.ToString()</td>
                <td>
                    <a href="javascript:void(0)" data-toggle="tooltip" data-placement="bottom" title="Detail" class="btn btn-xs btn-default observe-detail-btn"><i class="fa fa-folder-open"></i> </a>
                </td>
            </tr>
        }
    </tbody>
</table>
